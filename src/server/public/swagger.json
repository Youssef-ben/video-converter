	"paths": {
		"/server": {
			"get": {
				"operationId": "GetConfig",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/ApiResponse_ConfigDTO_"
								}
							}
						}
					}
				},
				"tags": [
					"Server"
				],
				"security": [],
				"parameters": []
			}
		},
		"/worker/fetch": {
			"get": {
				"operationId": "GetYoutubeVideoDetails",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/ApiResponse_YoutubeVideoDetails_"
								}
							}
						}
					}
				},
				"description": "Gets the details of the given youtube video.",
				"tags": [
					"worker"
				],
				"security": [],
				"parameters": [
					{
						"description": "The url of the Youtube video to be downloaded.",
						"in": "query",
						"name": "video_url",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				]
			}
		},
		"/worker/upload": {
			"post": {
				"operationId": "PostUploadVideoFile",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/ApiResponse_Record_string.string__"
								}
							}
						}
					}
				},
				"description": "Upload the video file to be converted.",
				"tags": [
					"worker"
				],
				"security": [],
				"parameters": []
			}
		},
		"/downloader": {
			"get": {
				"operationId": "GetDownloadVideo",
				"responses": {
					"200": {
						"description": "Returns the file to be downloaded.",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/ApiResponse_FileDownloadDetails_"
								}
							}
						}
					}
				},
				"description": "Download the the requested file with the desired format and name.",
				"tags": [
					"downloader"
				],
				"security": [],
				"parameters": [
					{
						"description": "The video key returned by the fetch action.",
						"in": "query",
						"name": "key",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"description": "The title of the video to be downloaded with.",
						"in": "query",
						"name": "title",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"description": "The type of the file [Audio || Video].",
						"in": "query",
						"name": "type",
						"required": true,
						"schema": {
							"$ref": "#/components/schemas/FileType"
						}
					}
				]
			}
		},
		"/security/login": {
			"post": {
				"operationId": "PostLogin",
				"responses": {
					"200": {
						"description": "Ok",
